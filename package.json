{
  "name": "wechaty-type-checker",
  "version": "0.2.7",
  "description": "make it simple to get wechaty type",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "pack": "npm pack",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint --ignore-pattern tests/fixtures/ '{bin,examples,scripts,src,tests}/**/*.ts'",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wechaty/wechaty-type-checker.git"
  },
  "keywords": [],
  "author": "Jiarui LI (李佳芮) <ruiruibupt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wechaty/wechaty-type-checker/issues"
  },
  "homepage": "https://github.com/wechaty/wechaty-type-checker#readme",
  "dependencies": {},
  "devDependencies": {
    "@chatie/eslint-config": "^0.8.1",
    "@chatie/git-scripts": "^0.6.1",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.10.1",
    "node-jq": "^1.11.0",
    "pkg-jq": "^0.2.4",
    "shx": "^0.3.2",
    "tstest": "^0.4.10"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  },
  "publishConfig": {
    "tag": "next"
  }
}
